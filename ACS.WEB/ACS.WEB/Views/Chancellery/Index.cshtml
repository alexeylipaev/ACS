@model IEnumerable<ACS.WEB.ViewModel.ChancelleryViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Канцелярия</h2>

<p>
    @Html.ActionLink("Создать новую запись", "Create")
</p>
<table class="table">
    <tr>
        <th>
            id
        </th>
        <th>
            Регистрационный номер
        </th>
        <th>
          Короткое описание
        </th>
        <th>
            Тип записи
        </th>
        <th>
            Дата регистрации
        </th>
        <th>
            Папка
        </th>
        <th>
            Журнал регистрации
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)    
            </td>
            <td>
              @Html.DisplayFor(modelItem => item.RegistrationNumber)  
            </td>
            <td>
              @Html.DisplayFor(modelItem => item.Summary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeRecordChancellery)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResponsibleEmployee_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateRegistration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FolderChancellery)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JournalRegistrationsChancellery.Name)
            </td>

            <td>
                @Html.ActionLink("Редактировать", "Edit", new { id = item.id }) |
                @Html.ActionLink("Подробнее", "Details", new { id = item.id }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>