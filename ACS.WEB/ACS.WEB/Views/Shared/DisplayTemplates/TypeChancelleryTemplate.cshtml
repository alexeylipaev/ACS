@using ACS.WEB.ViewModel;
@using ACS.WEB.Controllers;

@model TypeRecordChancelleryViewModel

    @foreach (TypeRecordChancelleryViewModel value in ChancelleryController.GetAllTypes())
    {
        if (value == Model)
        {
             <u>@value</u>@:&nbsp;
        }
        else
        {
            @value @:&nbsp;
        }
    }
